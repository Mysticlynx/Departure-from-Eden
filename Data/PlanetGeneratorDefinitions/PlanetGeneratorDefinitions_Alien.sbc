        <Latitude Min="0" Max="15"/>
        <SunAngleFromZenith Min="90" Max="180"/>
        <EnvironmentSound>AmbAlienDesertNight</EnvironmentSound>
      </SoundRule>
      <!-- Desert amb sound rule END -->
    </SoundRules>

